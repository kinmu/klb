	}
.ie8 #contents_portal .nav .master a,
.ie9 #contents_portal .nav .master a {
	-pie-background:linear-gradient(top, #c95893 0%, #ae065e 100%);
	filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#c95893', endColorstr='#ae065e');
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#c95893', endColorstr='#ae065e')";
	}
.ie8 #contents_portal .nav .questionnaire a,
.ie9 #contents_portal .nav .questionnaire a {
