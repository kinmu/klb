<%@ page language="java" contentType="text/css; charset=UTF-8" pageEncoding="UTF-8"%>
@charset "UTF-8";
.pie-element, .bordered, .gradient, #login, #login #entryTitle, #login #inputCon li .text, #login #error {
  behavior: url("../css/PIE.htc");
  position: relative;
}

.bordered {
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  -ms-border-radius: 5px;
  -o-border-radius: 5px;
  border-radius: 5px;
}

.gradient {
  background: url('data:image/svg+xml;base64,');
  background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #ff0000), color-stop(100%, #0000ff));
  background: -webkit-linear-gradient(#ff0000, #0000ff);
  background: -moz-linear-gradient(#ff0000, #0000ff);
  background: -o-linear-gradient(#ff0000, #0000ff);
  -pie-background: linear-gradient(#ff0000, #0000ff);
  background: linear-gradient(#ff0000, #0000ff);
}

.pie-container, .widget {
  z-index: 0;
  position: relative;
}

.z-pie-element, .widget h3 {
  behavior: url("../css/PIE.htc");
  z-index: 0;
}

.has-gradient {
  behavior: url("../css/PIE.htc");
  position: relative;
  background: url('data:image/svg+xml;base64,');
  background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #ff0000), color-stop(100%, #0000ff));
  background: -webkit-linear-gradient(#ff0000, #0000ff);
  background: -moz-linear-gradient(#ff0000, #0000ff);
  background: -o-linear-gradient(#ff0000, #0000ff);
  -pie-background: linear-gradient(#ff0000, #0000ff);
  background: linear-gradient(#ff0000, #0000ff);
}

/**
*	Login Container
* ************************************** */
#mainCon {
  position: absolute;
  top: 30%;
  left: 0%;
  width: 100%;
  margin: 0;
}

#post-login {
  position: relative;
  width: 50%;
  height: 100%;
  margin: 0 25% 0 25%;
}

#login {
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  -ms-border-radius: 5px;
  -o-border-radius: 5px;
  border-radius: 5px;
  background: #fff;
  -webkit-box-shadow: 0 1px 3px white;
  -moz-box-shadow: 0 1px 3px white;
  box-shadow: 0 1px 3px white;
}
#login #entryTitle {
  line-height: 3em;
  font-size: 123%;
  text-align: center;
  background: #3295D0;
  color: #fff;
  -webkit-border-radius: 5px 5px 0 0;
  -moz-border-radius: 5px 5px 0 0;
  -ms-border-radius: 5px 5px 0 0;
  -o-border-radius: 5px 5px 0 0;
  border-radius: 5px 5px 0 0;
  border-bottom: 1px solid #efefef;
  -webkit-box-shadow: inset 0 -1px 0 #1a6c9e;
  -moz-box-shadow: inset 0 -1px 0 #1a6c9e;
  box-shadow: inset 0 -1px 0 #1a6c9e;
}
#login #inputCon {
  padding: 16px 20px;
}
#login #inputCon li {
  padding: 0 0 16px;
}
#login #inputCon li label {
  display: none;
}
#login #inputCon li .text {
  width: 95%;
  padding: 0 2.5%;
  height: 2em;
  line-height: 2em;
  -webkit-border-radius: 1em;
  -moz-border-radius: 1em;
  -ms-border-radius: 1em;
  -o-border-radius: 1em;
  border-radius: 1em;
  border: 1px solid #ccc;
  outline: none;
}
#login #messageCon {
  padding: 0px 20px;
}
#login #submitCon {
  padding: 16px 20px;
  text-align: right;
}
#login #error {
  margin-top: 8px;
  line-height: 2em;
  color: #f63;
  text-align: center;
  -webkit-border-radius: 1em;
  -moz-border-radius: 1em;
  -ms-border-radius: 1em;
  -o-border-radius: 1em;
  border-radius: 1em;
}
#login #error.hide {
  display: none;
}
#login #error.show {
  display: block;
}
#login #error:before {
  display: -moz-inline-stack;
  display: inline-block;
  vertical-align: middle;
  *vertical-align: auto;
  zoom: 1;
  *display: inline;
  padding: 0px 8px 0px 0;
  font-size: 125%;
  color: #f00;
}
