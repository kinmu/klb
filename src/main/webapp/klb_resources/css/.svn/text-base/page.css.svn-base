<%@ page language="java" contentType="text/css; charset=UTF-8" pageEncoding="UTF-8"%>
@charset "UTF-8";
/* clearfix */
.clearfix, #page, #navi_info, .nav_menu {
	*zoom: 1;
}
.clearfix:after, #page:after, #navi_info:after, .nav_menu:after {
	content: "";
	display: table;
	/*clear: both;   -------- lim */
}

/* ====================
layout
==================== */

/* header */
#navi_info {
	height: 44px;
}
#navi {
	margin:0 0 10px;
	padding:0;
	background-color: #fff;
	-webkit-box-shadow: 0px 1px 2px 2px rgba(153, 153, 153, 0.4);
	-moz-box-shadow: 0px 1px 2px 2px rgba(153, 153, 153, 0.4);
	-ms-box-shadow: 0px 1px 2px 2px rgba(153, 153, 153, 0.4);
	box-shadow: 0px 1px 2px 2px rgba(153, 153, 153, 0.4);
}
.logo, #bread, .user_info {
	padding: 7px 7px 0;
}
.logo {
	height:44px;
	width:195px;
	float: left;
	text-align: center;
	padding: 0;
	margin: 0;
	font-size:0;
}
#bread {
	float: left;
	height:25px;
	padding-top: 7px;
}
#bread li {
	float: left;
	padding: 0 5px 0;
}
#bread li:after {
	content: " >";
}
#bread li.last:after {
	content: "";
}
.user_info {
	position: absolute;
	right: 107px;
}
.user_info .date, .user_info .school, .user_info .name {
	padding: 0 1em 0 0;
	font-weight: bold;
	font-size: 115%;
}
.account {
	position: absolute;
	right: 0;
}

.bg_navi_menu {
	width: 100%;
	height: 44px;
	background: #ff9721;
	min-width:1030px;
}

.nav_menu {
	display:table;
	background: #4da2d5\9;
	display:block\9;
}
.nav_menu ul,
.nav_menu ul ul {
	background: #FFF;
	padding: 0;
}

.navi_menu {
	float: left;
	line-height: 1;
}
.nav_menu li {
	position: relative;
}
.navi_menu a {
	clear: both;
	background: #4da2d5;
	background:
		url('data:image/svg+xml;base64,');
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0.88, #7dc9f7), color-stop(0.00, #34a0e0) );
	background: -webkit-linear-gradient(top, #34a0e0 0%, #7dc9f7 88%);
	background: -moz-linear-gradient(top, #34a0e0 0%, #7dc9f7 88%);
	background: -ms-linear-gradient(top, #34a0e0 0%, #7dc9f7 88%);
	background: linear-gradient(to bottom, #34a0e0 0%, #7dc9f7 88%);

	display: block;
	padding: 10px 20px;
	text-decoration: none;
	color: #fff;
	font-size: 110%;
	font-weight: bold;
	text-shadow: 1px 1px 4px rgba(0, 0, 0, 0.5);
}
.navi_menu a:hover {
	background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #9ed2f0), color-stop(100%, #329edf));
	background: -webkit-linear-gradient(top, #9ed2f0, #329edf);
	background: -moz-linear-gradient(top, #9ed2f0, #329edf);
	background: -o-linear-gradient(top, #9ed2f0, #329edf);
	background: -ms-linear-gradient(top, #9ed2f0, #329edf);
	// -pie-background: linear-gradient(top, #286dc7, #235ba5);
	background: linear-gradient(top, #9ed2f0,#329edf);
	filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#9ed2f0', endColorstr='#329edf');
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#9ed2f0', endColorstr='#329edf')";
}

.navi_menu a.selected {
	background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #286dc7), color-stop(100%, #235ba5));
	background: -webkit-linear-gradient(top, #286dc7, #235ba5);
	background: -moz-linear-gradient(top, #286dc7, #235ba5);
	background: -o-linear-gradient(top, #286dc7, #235ba5);
	background: -ms-linear-gradient(top, #286dc7, #235ba5);
	// -pie-background: linear-gradient(top, #286dc7, #235ba5);
	background: linear-gradient(top, #286dc7,#235ba5);
	filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#286dc7', endColorstr='#235ba5');
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#286dc7', endColorstr='#235ba5')";
}

.navi_menu li a,
.navi_menu li a:hover,
.navi_menu li a.selected {
	background: #fff;
	-pie-background: none;
	background: none;
	filter: none;
	-ms-filter:none;
	text-shadow: none;
	width: 190px;
}

/* dropdown */
.nav_menu li:hover > ul {
	display: block;
	-webkit-box-shadow: 0px 1px 2px 2px rgba(153, 153, 153, 0.4);
	-moz-box-shadow: 0px 1px 2px 2px rgba(153, 153, 153, 0.4);
	-ms-box-shadow: 0px 1px 2px 2px rgba(153, 153, 153, 0.4);
	box-shadow: 0px 1px 2px 2px rgba(153, 153, 153, 0.4);
}

/* level 2 list */
.nav_menu ul {
	display: none;
	margin: 0;
	padding: 0;
	position: absolute;
	top: 41px;
	left: 0;
	border:#FF9721 solid 2px;
	background: #fff;
}

.nav_menu .navi_menu ul li {
	float: none;
	width: 200px;
	margin: 0;
	padding: 0;
	background: #fff;
	border-bottom: #ccc solid 1px;
}

.nav_menu .navi_menu ul li a {
	background: #fafafa;
	color: #000;
	font-size:12px;
	padding: 5px;
}

.nav_menu .navi_menu ul li a:hover{
	background: #286dc7;
	color: #FFF;
}

.nav_menu .navi_menu ul li a:before {
	content: "隨�ｿｽ";
	color: #FF9721;
	font-size:9px;
}

/* level 3+ list */
.nav_menu .navi_menu ul ul {
	width: 200px;
	top: -3px;
	left: 199px;
}
.nav_menu .navi_menu ul ul li {
	float: none;
	width: 200px;
	margin: 0;
	padding: 0;
}

/* clearfix */
.nav_menu:after {
	content: ".";
	display: block;
	clear: both;
	visibility: hidden;
	line-height: 0;
	height: 0;
}
.nav_menu {
	display: inline-block;
}
html[xmlns] .nav_menu {
	display: block;
}

* html .nav_menu {
	height: 1%;
}


