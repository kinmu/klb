span.ico.web {background:url(../images/icon_small_web.png) 0 0 no-repeat;}
span.ico.img {background:url(../images/icon_small_picture.png) 0 0 no-repeat;}
span.ico.scorm {background:url(../images/icon_small_scorm.png) 0 0 no-repeat;}
span.ico_close {display:inline-block; margin-left:2px; padding:0 4px; background-color:#ef2148; border-radius:3px; font-size:0.75em; font-weight:bold; color:#fff; cursor:pointer;}

.round_box {position:relative; border:1px solid #999; border-radius:6px; background-color:#fff; padding:10px 30px;}
.round_box:after {content:""; display:block; clear:both;}
.round_box input[type="text"] {height:18px; line-height:18px;}
.round_box input[type="button"], .round_box input[type="submit"], .round_box input[type="cancel"] {height:22px; padding:0 10px; line-height:22px !important;}
.round_box .btn {position:absolute; bottom:9px; right:12px;}
.src_box {border:1px solid #808080; background-color:#fff; padding:5px 7px;}
.src_box input[type="text"] {height:18px; line-height:18px;}
.src_box input[type="button"], .src_box input[type="submit"], .src_box input[type="cancel"] {height:22px; padding:0 10px; line-height:22px !important;}

.chk_list {overflow:hidden;}
.chk_list li {float:left; width:33%; margin:5px 0;}

.small_font {font-size:9pt;}

.list_type1 {list-style:disc; padding-left:20px;}
.list_type1 li:first-child {margin-top:5px;}
.list_type2 {list-style:disc; padding-left:20px;}
.list_type2 li {margin:0 !important;}

.textbook {width:100%; padding:30px 0; text-align:left;}
.textbook li {
	display:inline-block;
	margin:10px;
	padding:5px 10px;
	border:1px solid #ccc;
	border-radius:5px;
	background:-webkit-linear-gradient(#f9f9f9, #f1f1f1);
	background:-moz-linear-gradient(#f9f9f9, #f1f1f1);
	background:-o-linear-gradient(#f9f9f9, #f1f1f1);
	background:linear-gradient(#f9f9f9, #f1f1f1);
	-pie-background:linear-gradient(#f9f9f9, #f1f1f1);
	box-shadow:1px 2px 3px #ddd;
	text-align:center;
}
.textbook li a {display:inline-block;width:120px; padding-top:80px; background:url(../images/ico_lecture_book.png) center 0 no-repeat; text-align:center;}
.textbook.book li a {background:url(../images/klb_a_icon_book.png) center 0 no-repeat;}
.textbook.exam li a {background:url(../images/klb_a_icon_exam.png) center 0 no-repeat;}
.textbook.homework li a {background:url(../images/klb_a_icon_homework.png) center 0 no-repeat;}
.textbook.discussion li a {background:url(../images/klb_a_icon_discussion.png) center 0 no-repeat;}

table.test_cndn {font-size:1.4em;}
table.test_cndn .subject {font-size:1.8em;}
table.test_cndn input[type="button"] {padding:10px 20px; font-size:1.4em; color:#0000ee;}

input.datepicker {width:120px !important;}

.attendance {border:1px solid #808080; background-color:#fff; padding:10px 15px;}
.attendance .result_group {margin-bottom:10px; border-bottom:1px solid #808080;}
.attendance .ico_attend {display:inline-block; width:12px; height:12px; margin-right:5px; margin-left:15px; border:1px solid #666; background-color:#999; vertical-align:middle;}
.attendance .color1 {background-color:#4f81bd !important;}
.attendance .color2 {background-color:#ffc000 !important;}
.attendance .color3 {background-color:#f79646 !important;}
.attendance .member:after {content:""; display:block; clear:both;}
.attendance .member dl {position:relative; float:left; width:12%; height:85px; margin:0 1% 10px; border:1px solid #666; background-color:#999;}
.attendance .member dt, .attendance .member dd {text-align:center;}
.attendance .member dt {position:absolute; top:6px; width:100%; font-weight:bold;}
.attendance .member dd {position:absolute; bottom:4px; width:100%;}
.attendance .member .color1 {color:#fff;}

.plus_minus a {display:inline-block; width:20px; height:20px; border:1px solid #ccc; background-color:#fff; font-size:16px; font-weight:bold; line-height:20px; color:blue; text-decoration:none; text-align:center;}
.plus_minus a:hover {background-color:#126fd8; color:#fff;}

.diagram {height:200px;}

.sub_title {display:inline-block; width:100px; font-weight:bold;}

.block1 {padding-top:10px; padding-bottom:15px;}
.block1 .upload:after {content:""; display:block; clear:both;}
.block1 .clearfix textarea {width:100%;}
.block1 .upload textarea {float:left; width:80%; height:100px;}
.block1 .upload .upload_inner {float:right; width:18%; height:97px; border:3px solid #6e8be0; background-color:#f1f1f1; z-index:10;}
.block1 .upload .upload_inner a {display:block; height:100%;}
.block1 .upload .upload_inner .upload_list {display:none; width:60%; margin-left:-3px; margin-top:4px; padding:2px; border:1px solid #333; background-color:#fff; z-index:10;}
.block1 .upload .upload_inner .upload_list a {display:block; height:20px; padding-left:5px; font-size:12px; color:#4c7bda; line-height:20px; text-decoration:none;}
.block1 .upload .upload_inner .upload_list a:hover {background-color:#d3e0fb;}

.fixed_area .plus_minus {margin-right:5px;}
.fixed_area .plus_minus a {font-size:14px;}
#contents_portal .time_table1 td {height:64px !important;}
#contents_portal .time_table1 td ul {height:59px; padding-top:5px;}
#contents_portal .time_table1 td ul li {}
#contents_portal .time_table1 td ul li.end {color:#ff0000;}
#contents_portal .fixed_area .timetable .time_table1 td a {font-size:13px; color:#333; font-weight:normal; line-height:1.4em;}

.check_class {position:relative; padding:0 50px 3px;}
.check_class .btn_prev {position:absolute; top:0; left:0;}
.check_class .btn_next {position:absolute; top:0; right:0;}

.attendance_blue {background:#4f81bd !important;}
.attendance_orange {background:#ffc000 !important;}
.attendance_red {background:#f79646 !important;}

.attach_file_del_btn {vertical-align:middle;}
.attach_file_input {width:99.5%;}
.attach_file_list {margin-bottom:5px;}

.ellipsis {	white-space: nowrap; overflow: hidden; text-overflow: ellipsis; }
.ellipsis.multiline { white-space: normal; }

.partellipsisleft {
	width:90%;
	overflow : hidden;
	white-space: nowrap;
	text-overflow:ellipsis;
	-o-text-overflow:ellipsis;
	-ms-text-overflow:ellipsis;
	text-align:left;
}

.partellipsiscenter {
	width:90%;
	overflow : hidden;
	white-space: nowrap;
	text-overflow:ellipsis;
	-o-text-overflow:ellipsis;
	-ms-text-overflow:ellipsis;
	text-align:center;
}