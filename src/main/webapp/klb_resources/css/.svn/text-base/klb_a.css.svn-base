/* contents_portal ================================ */
/* subject */
#contents_portal .subject h1 {
	position:relative;
	height:33px;
	margin-bottom:12px;
	padding:4px;
	background:-webkit-gradient(linear, left top, left bottom, color-stop(1.00, #fdb60b), color-stop(0.00, #feca50));
	background:-webkit-linear-gradient(#feca50, #fdb60b);
	background:-moz-linear-gradient(#feca50, #fdb60b);
	background:-ms-linear-gradient(#feca50, #fdb60b);
	background:linear-gradient(#feca50, #fdb60b);
	font-size:14px;
	font-weight:normal;
	line-height:33px;
	text-align:left;
	padding-left:50px;
	padding-right:10px;
	text-overflow:ellipsis;
	overflow:hidden;
	white-space:nowrap;
	}
#contents_portal .subject .h1gray {background-image: linear-gradient(rgb(189, 189, 189), rgb(140, 140, 140));}
#contents_portal .subject h1 div {position:absolute; top:6px; font-size:14px;}
#contents_portal .subject h1 div span {float:left; display:inline-block; width:29px; height:29px;}
#contents_portal .subject h1 div span a {display:inline-block; width:29px; height:29px;}
#contents_portal .subject h1 .left {left:9px;}
#contents_portal .subject h1 .right {right:9px;}
#contents_portal .subject h2 {
	font-size:25px;
	font-weight:normal;
	color:#fe8804;
	text-align:center;
	}

#contents_portal .subject .teaching {
	margin:0 10px;
	padding:15px 0 0;
	min-height:475px;
	height:auto;
	border:1px solid #ddd;
	overflow:hidden;
	}

#contents_portal .subject .teaching li {
	margin:0 0 15px 13px;
	display:inline-block;
	width:115px;
	text-align:center;
	-webkit-border-radius:5px;
	-moz-border-radius:5px;
	-ms-border-radius:5px;
	border-radius:5px;
	border:1px solid #ddd;
	-webkit-box-shadow:0px 1px 2px 2px rgba(223, 223, 223, 0.3);
	-moz-box-shadow:0px 1px 2px 2px rgba(223, 223, 223, 0.3);
	box-shadow:0px 1px 2px 2px rgba(223, 223, 223, 0.3);
	background:-webkit-linear-gradient(#fff, #fafafa);
	background:-moz-linear-gradient(#fff, #fafafa);
	background:linear-gradient(#fff, #fafafa);
	-pie-background:linear-gradient(#fff, #fafafa);
	behavior:url(../css/PIE.htc);
}
#contents_portal .subject .teaching li a {
	padding:80px 5px 20px;
	display:block;
	font-size:12px;
	color:#000;
	text-decoration:none;
	-webkit-border-radius:5px;
	-moz-border-radius:5px;
	-ms-border-radius:5px;
	border-radius:5px;
	}
#contents_portal .subject .book a {background:url(../images/klb_a_icon_book.png) no-repeat center 13px;}
#contents_portal .subject .bookbest a {background:url(../images/klb_a_icon_bookbest.png) no-repeat center 13px;}
#contents_portal .subject .exam a {background:url(../images/klb_a_icon_exam.png) no-repeat center 13px;}
#contents_portal .subject .exambest a {background:url(../images/klb_a_icon_exambest.png) no-repeat center 13px;}
#contents_portal .subject .homework a {background:url(../images/klb_a_icon_homework.png) no-repeat center 13px;}
#contents_portal .subject .discussion a {background:url(../images/klb_a_icon_discussion.png) no-repeat center 13px;}
#contents_portal .subject .web a {background:url(../images/klb_a_icon_web.png) no-repeat center 13px;}
#contents_portal .subject .youtube a {background:url(../images/klb_a_icon_youtube.png) no-repeat center 13px;}

/* sidemenu_portal ================================ */
/* admin */
#sidemenu_portal .admin .adminnav li a {
	padding:6px;
	display:block;
	background: url('data:image/svg+xml;base64,');
	background:-webkit-gradient(linear, left top, left bottom, color-stop(1.00, #f7f7f7), color-stop(0.00, #fff));
	background:-webkit-linear-gradient(#fff, #f7f7f7);
	background:-moz-linear-gradient(#fff, #f7f7f7);
	background:-ms-linear-gradient(#fff, #f7f7f7);
	background:linear-gradient(#fff, #f7f7f7);
	filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#ffffff', endColorstr='#f7f7f7');
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#ffffff', endColorstr='#f7f7f7')";
	-webkit-border-radius:5px;
	-moz-border-radius:5px;
	-ms-border-radius:5px;
	border-radius:5px;
	}

/* learn */
#sidemenu_portal .learn ul {
	padding:1px 2px 1px;
	background-color:#fff;
	-webkit-border-radius:5px;
	-moz-border-radius:5px;
	-ms-border-radius:5px;
	border-radius:5px;
	-webkit-box-shadow:0px 1px 5px 2px rgba(0, 0, 0, 0.2);
	-moz-box-shadow:0px 1px 5px 2px rgba(0, 0, 0, 0.2);
	box-shadow:0px 1px 5px 2px rgba(0, 0, 0, 0.2);
	overflow:hidden;
	}
#sidemenu_portal .learn li {
	margin:1px 0;
	border-bottom:1px solid #dbdbdd;
	}
#sidemenu_portal .learn li:last-child {border-bottom:none;}
#sidemenu_portal .learn li a {
	margin:1px 0;
	padding:20px 10px 20px 10px;
	display:block;
	background:-webkit-gradient(linear, left top, left bottom, color-stop(1.00, #f9f9f9), color-stop(0.00, #fff));
	background:-webkit-linear-gradient(#fff, #f9f9f9);
	background:-moz-linear-gradient(#fff, #f9f9f9);
	background:-ms-linear-gradient(#fff, #f9f9f9);
	background:linear-gradient(#fff, #f9f9f9);
	font-size:16px;
	text-decoration:none;
	line-height:1.25;
	border-left:4px solid #d0d0d1;
	}
#sidemenu_portal .learn li a:before {
	content: "�ｽ�ｽ;
	display:table-cell;
	vertical-align:middle;
	font-weight:bold;
	color:#666;
	}
#sidemenu_portal .learn li a span {
	display:table-cell;
	color:#333;
	}
#sidemenu_portal .learn li.act a {
	background:-webkit-gradient(linear, left top, left bottom, color-stop(1.00, #ffebd5), color-stop(0.00, #fffefd));
	background:-webkit-linear-gradient(#fffefd, #ffebd5);
	background:-moz-linear-gradient(#fffefd, #ffebd5);
	background:-ms-linear-gradient(#fffefd, #ffebd5);
	background:linear-gradient(#fffefd, #ffebd5);
	border-left:4px solid #ff8f11;
	color:#ff8f11;
	}
#sidemenu_portal .learn li.act a:before {color:#ff8f11;}
#sidemenu_portal .learn.type1 li a {padding:5px 10px;}

/* modal ========================================== */
/* modal-table */
.modal-table {
	margin:0;
	padding:0;
	font-size:12px;
	color:#111;
	border:1px solid #d5d5d5;
	border-right:none;
	border-collapse:separate;
	}
.modal-table td p,
.modal-table td ul,
.modal-table td li {
	margin:0;
	padding:0;
	}
.modal-table td ul,
.modal-table td li {list-style-type:none;}
.modal-table th {
	padding:5px 10px;
	background-color:#f2f2f2;
	font-weight:bold;
	text-align:center;
	border-top:1px solid #fff;
	border-right:1px solid #d5d5d5;
	border-bottom:1px solid #d5d5d5;
	border-left:1px solid #fff;
	table-layout:fixed;
	height:28px;
	}
.modal-table thead tr:first-child th {border-bottom:1px solid #d5d5d5;}
.modal-table td {
	padding:10px;
	border-right:1px solid #d5d5d5;
	border-bottom:1px solid #d5d5d5;
	table-layout:fixed;
	height:18px;
	}
.modal-table td input[type="text"], .modal-table td input[type="password"], .modal-table td textarea {width:99%;}
.modal-table td .words {display:block; margin-top:5px;}
.modal-table td .words strong {margin-left:5px; font-weight:normal;}
.modal-table td .words strong em {font-weight:bold;}
.modal-table td.descript { background-color:#d9fbb0;}
.modal-table tr:last-child th,
.modal-table tr:last-child td {border-bottom:none;}
.modalBtn li {
	margin:0;
	padding:0;
	display:table-cell;
	list-style-type:none;
	}
.modalBtn li input {margin:0;}

/* online */
.online.reveal-modal {
	border:#fff solid 2px;
	background-color: #fff;
}
.online .modal-contents {
	margin:0;
	padding:0;
	height:591px;
	overflow-y:scroll;
	-webkit-overflow-scrolling:touch;
	}
.online .modal-online {width:862px;}
.online .modal-online th {width:149px;}

.online .modal-online td .disc li {
	margin-bottom:0.5em;
	padding-left:1em;
	text-indent:-1em;
	}
.online .modal-online td .disc li:last-child {margin-bottom:0;}

.online .modalBtn ul {
	margin:0 auto;
	padding:0;
	display:table;
	}
.online .modalBtn li input {margin:0 10px;}

.mt20 {margin-top:20px;}

/* exercise */
.reveal-modal {
	border:#fff solid 2px;
	background-color: #fff;
}
.reveal-modal .modal-test {width:100%;}
.reveal-modal .modal-test th {
	width:129px;
	font-size:32px;
	text-transform:uppercase;
	vertical-align:middle;
	}
.reveal-modal .modal-test tr:first-child td:last-child {
	width:345px;
	text-align:right;
	vertical-align:bottom;
	}

.reveal-modal .modal-contents {
	margin:20px 0 0;
	padding:0;
	height:471px;
	overflow-y:scroll;
	-webkit-overflow-scrolling:touch;
	}
.reveal-modal .modal-question {width:862px;}
.reveal-modal .modal-question th {width:129px;}

.reveal-modal .modal-question01 {
	margin-top:13px;
	width:862px;
	}
.reveal-modal .modal-question01 img {
	display:inline-block;
	width:284px;
	height:auto;
	vertical-align:top;
	}
.reveal-modal .modal-question01 .movie {display:inline-block; width:284px; vertical-align:top;}
.reveal-modal .modal-question01 .modal-q01 {
	margin-left:4px;
	display:inline-block;
	width:569px;
	}
.reveal-modal .modal-question01 .modal-q01 th {
	width:79px;
	background-color:#fff;
	font-weight:normal;
	}
.reveal-modal .modal-question01 .modal-q01 tr:first-child th {
	background-color:#f2f2f2;
	font-weight:bold;
	}
.reveal-modal .modal-question01 .modal-q01 td {width:447px;}

.reveal-modal .modalBtn ul {
	margin:0 0 0 auto;
	padding:0;
	display:table;
	}
.reveal-modal .modalBtn li input {margin:0 -10px 0 30px;}

