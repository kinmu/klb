.file_input span {display:inline-block; margin-left:5px; vertical-align:middle;}
.file_input span a {font-size:18px; font-weight:bold; color:#fff; text-decoration:none;}
.file_input span a:hover {text-decoration:underline;}
.file_input span.type1 a {cursor:text;}
.file_input span.type1 a:hover {text-decoration:none;}

.message_read {padding:10px 10px;}
.message_read dl {overflow:hidden; background-color:#e6e8eb;}
.message_read dl dt, .message_read dl dd {float:left;}
.message_read dl dd {margin-left:10px;}
.message_read p {margin-top:20px;}
.message_read .result_group {margin-top:20px;}
.message_read dl.qaa {position:relative; margin-bottom:5px; padding-bottom:5px; padding-left:0px; border-bottom:1px solid #999; background-color:inherit;}
.message_read dl.qaa.a {padding-top:15px; border-top:2px solid #666;}
.message_read dl.qaa dt, .message_read dl.qaa dd {float:none;}
.message_read dl.qaa dt {font-weight:bold;}
.message_read dl.qaa .question {
	position:absolute;
	left:0;
	display:block;
	width:50px;
	height:40px;
	border:1px solid #61ad05;
	border-radius:10px;
	background-color:#6cb70c;
	background:-webkit-linear-gradient(#85ce1a, #5ca701);
	background:-moz-linear-gradient(#85ce1a, #5ca701);
	background:-o-linear-gradient(#85ce1a, #5ca701);
	background:linear-gradient(#85ce1a,#5ca701);
	text-align:center;
	font-size:2em;
	color:#fff;
	line-height:36px;
}
.message_read dl.qaa .answer {
	position:absolute;
	left:0;
	display:block;
	width:50px;
	height:40px;
	border:1px solid #4a4b4c;
	border-radius:10px;
	background-color:#67686a;
	background:-webkit-linear-gradient(#7d7e7f, #595a5c);
	background:-moz-linear-gradient(#7d7e7f, #595a5c);
	background:-o-linear-gradient(#7d7e7f, #595a5c);
	background:linear-gradient(#7d7e7f,#595a5c);
	text-align:center;
	font-size:2em;
	color:#fff;
	line-height:36px;
}
.message_read dl.qaa dd {margin-left:0; color:#999;}
.message_read dl.qaa dd strong {margin-right:10px; color:#666; font-weight:normal;}
.message_read dl.qaa .recommend {position:absolute; right:0; bottom:6px; padding:0 5px; font-size:9pt; color:#333; font-weight:bold;}
.message_read dl.qaa .recommend span {width:auto; margin-left:10px;}
.message_read dl.qaa .recommend span:first-child {margin-left:0;}
.message_read dl.qaa .recommend span strong {margin-right:0;}
.message_read ol {list-style:decimal; margin-top:5px; margin-left:22px;}
.message_read .write {margin-top:10px; padding-top:10px; padding-bottom:10px; border-top:1px solid #666;}
.message_read .write table {width:100%; table-layout:fixed; margin:0; padding:0;}
.message_read .write table td {margin:0; padding:0; border:none; text-align:left;}
.message_read .write table td.end {text-align:right;}
.message_read .write input[type="text"] {width:100%; height:24px;}
.message_read .write input[type="button"] {width:60px; height:27px; margin:0; padding:0;}
.message_read .reply_wrap {padding:10px 15px; border:1px solid #dcd6c9; background-color:#f5efe1; color:#333;}
.message_read .reply_wrap .total {position:relative; padding-bottom:2px; border-bottom:1px solid #666; font-weight:bold; z-index:10;}
.message_read .reply dl {position:relative; margin-top:-1px; padding:5px 0; border-top:1px dashed #999; background-color:inherit; z-index:9;}
.message_read .reply dl dt {float:none; font-size:9pt; color:#999;}
.message_read .reply dl dt strong {color:#666; font-weight:normal;}
.message_read .reply dl dd {float:none; margin-left:0; font-size:9pt; color:#333; font-weight:bold; word-break: break-all;}
.message_read .reply .write {margin-top:0; padding-left:40px; border-top:1px solid #999; background:url(../images/ico_reply.gif) 15px 10px no-repeat;}
.message_read .reply .link {position:absolute; right:2px; top:6px; font-weight:normal; color:#bbb;}
.message_read .reply .link a {width:auto;}
.message_read .reply a {display:inline-block;}
/**** Reply 1 Depth ****/
.message_read .reply .reply dl {background:url(../images/ico_reply.gif) 15px 10px no-repeat;}
.message_read .reply .reply dt, .message_read .reply .reply dd {padding-left:40px;}
.message_read .reply .reply .write {padding-left:80px; background-position:55px 10px;}
/**** Reply 2 Depth ****/
.message_read .reply .reply .reply dl {background:url(../images/ico_reply.gif) 55px 10px no-repeat;}
.message_read .reply .reply .reply dt, .message_read .reply .reply .reply dd {padding-left:80px;}
.message_read .reply .reply .reply .write {padding-left:120px; background-position:95px 10px;}
/**** Reply 3 Depth ****/
.message_read .reply .reply .reply .reply dl {background:url(../images/ico_reply.gif) 95px 10px no-repeat;}
.message_read .reply .reply .reply .reply dt, .message_read .reply .reply .reply .reply dd {padding-left:120px;}
.message_read .reply .reply .reply .reply .write {padding-left:160px; background-position:135px 10px;}
/**** Reply 4 Depth ****/
.message_read .reply .reply .reply .reply .reply dl {background:url(../images/ico_reply.gif) 135px 10px no-repeat;}
.message_read .reply .reply .reply .reply .reply dt, .message_read .reply .reply .reply .reply .reply dd {padding-left:160px;}
.message_read .reply .reply .reply .reply .reply .write {padding-left:200px; background-position:175px 10px;}
/**** Reply 5 Depth ****/
.message_read .reply .reply .reply .reply .reply .reply dl {background:url(../images/ico_reply.gif) 175px 10px no-repeat;}
.message_read .reply .reply .reply .reply .reply .reply dt, .message_read .reply .reply .reply .reply .reply .reply dd {padding-left:200px;}
.message_read .reply .reply .reply .reply .reply .reply .write {padding-left:240px; background-position:215px 10px;}
/**** Reply 6 Depth ****/
.message_read .reply .reply .reply .reply .reply .reply .reply dl {background:url(../images/ico_reply.gif) 215px 10px no-repeat;}
.message_read .reply .reply .reply .reply .reply .reply .reply dt, .message_read .reply .reply .reply .reply .reply .reply .reply dd {padding-left:240px;}
.message_read .reply .reply .reply .reply .reply .reply .reply .write {padding-left:280px; background-position:255px 10px;}
.message_read.src_box {margin-top:-1px;}
.message_read.src_box:first-child {margin-top:0;}

.vote { position:absolute; top:40px; width:100%; height:61px; background-color:#e7d3ab; text-align:center;}
.vote a {
	display:inline-block;
	width:150px;
	height:30px;
	margin:14px 5px 0;;
	border:2px solid #af9053;
	border-radius:10px;
	background:-webkit-linear-gradient(#fff, #fafafa);
	background:-moz-linear-gradient(#fff, #fafafa);
	background:linear-gradient(#fff, #fafafa);
	-pie-background:linear-gradient(#fff, #fafafa);
	behavior:url(../css/PIE.htc);
	box-shadow:0px 1px 2px #b5b5b5;
	font-size:14px;
	font-weight:bold;
	line-height:32px;
	text-decoration:none;
}
.vote span {display:inline-block; margin:16px 10px 0; font-size:14px;}

.check_list {overflow:hidden;}
.check_list li {float:left; display:inline-block; width:auto; margin-left:10px;}
.check_list li:first-child {margin-left:5px;}
.check_list input {margin:0; vertical-align:middle;}
.check_list label {vertical-align:middle;}

a.message_input {padding-left:25px; background:url(../images/ico_message_input.png) 1px 6px no-repeat;}
a.message_output {padding-left:25px; background:url(../images/ico_message_output.png) 1px 6px no-repeat;}

.survey {margin:20px 10px;}
.survey li.question {padding:8px 5px; border-bottom:1px dashed #999; font-size:14px; font-weight:bold; list-style:decimal; list-style-position:inside;}
.survey li.question:first-child {padding-top:0;}
.survey li.question li.que_typ3 {width:95%;}
.survey li.question li.que_typ3 input {width:100%;}
.survey li.question .que_typ3_rplconts {display:none;margin-left:10px;width:647px;}
.survey ul {margin-top:5px;}
.survey ul li {display:inline-block; width:auto; margin-left:20px; font-size:13px; font-weight:normal;}
.survey ul li:first-child {margin-left:10px;}
.survey .diagram {height:150px; background-color:yellow;}

.ico {display:inline-block; width:20px; height:20px; margin-right:4px; font-size:10px; font-weight:bold; color:#fff; line-height:20px; text-align:center; vertical-align:middle;}
.ico.new {width:30px; height:18px; border-radius:4px; background-color:red; line-height:18px;}
.ie8 .ico.new,
.ie9 .ico.new {margin-bottom:2px;}

.modal-table .mutiple {width:100%; height:400px;}

img.ui-datepicker-trigger {
    border: 1px solid dimgray;
    margin-left: 4px;
    vertical-align: middle;
}