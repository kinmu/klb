<%@ page language="java" contentType="text/css; charset=UTF-8" pageEncoding="UTF-8"%>
.pie-element, .bordered, .gradient, #timeTable-control li, #timeTable-control li div, #timeTable.check th, #timeTable.check td, #timeTable.check .chk-group label, #timeTable.check .area {
  behavior: url("../css/PIE.htc");
  position: relative;
}

.bordered {
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  -ms-border-radius: 5px;
  -o-border-radius: 5px;
  border-radius: 5px;
}

.gradient {
  background: url('data:image/svg+xml;base64,');
  background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #ff0000), color-stop(100%, #0000ff));
  background: -webkit-linear-gradient(#ff0000, #0000ff);
  background: -moz-linear-gradient(#ff0000, #0000ff);
  background: -o-linear-gradient(#ff0000, #0000ff);
  -pie-background: linear-gradient(#ff0000, #0000ff);
  background: linear-gradient(#ff0000, #0000ff);
}

.pie-container, .widget {
  z-index: 0;
  position: relative;
}

.z-pie-element, .widget h3 {
  behavior: url("../css/PIE.htc");
  z-index: 0;
}

.has-gradient {
  behavior: url("../css/PIE.htc");
  position: relative;
  background: url('data:image/svg+xml;base64,');
  background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #ff0000), color-stop(100%, #0000ff));
  background: -webkit-linear-gradient(#ff0000, #0000ff);
  background: -moz-linear-gradient(#ff0000, #0000ff);
  background: -o-linear-gradient(#ff0000, #0000ff);
  -pie-background: linear-gradient(#ff0000, #0000ff);
  background: linear-gradient(#ff0000, #0000ff);
}

/**
*	Select diagram
* *********************************************** */
.timeTable-nav {
  margin-top: 16px;
}

#timeTable-control {
  *zoom: 1;
  display: -moz-inline-stack;
  display: inline-block;
  vertical-align: middle;
  *vertical-align: auto;
  zoom: 1;
  *display: inline;
}
#timeTable-control:after {
  content: "";
  display: table;
  clear: both;
}
#timeTable-control li {
  float: left;
  text-align: center;
  border-right: 1px solid #949494;
}
#timeTable-control li.first {
  -webkit-border-radius: 5px 0 0 0px;
  -moz-border-radius: 5px 0 0 0px;
  -ms-border-radius: 5px 0 0 0px;
  -o-border-radius: 5px 0 0 0px;
  border-radius: 5px 0 0 0px;
  border-left: 1px solid #000;
}
#timeTable-control li.first .time {
  -webkit-border-radius: 5px 0 0 0px;
  -moz-border-radius: 5px 0 0 0px;
  -ms-border-radius: 5px 0 0 0px;
  -o-border-radius: 5px 0 0 0px;
  border-radius: 5px 0 0 0px;
}
#timeTable-control li.last {
  -webkit-border-radius: 0px 5px 0 0px;
  -moz-border-radius: 0px 5px 0 0px;
  -ms-border-radius: 0px 5px 0 0px;
  -o-border-radius: 0px 5px 0 0px;
  border-radius: 0px 5px 0 0px;
  border-right: none;
}
#timeTable-control li.last .time {
  -webkit-border-radius: 0px 5px 0 0px;
  -moz-border-radius: 0px 5px 0 0px;
  -ms-border-radius: 0px 5px 0 0px;
  -o-border-radius: 0px 5px 0 0px;
  border-radius: 0px 5px 0 0px;
}
#timeTable-control li a {
  display: block;
  text-decoration: none;
  color: #fff;
}
#timeTable-control li a:hover div {
  background: #9f9f9f;
}
#timeTable-control li div {
  padding: 0 16px;
  line-height: 2em;
  -webkit-box-shadow: inset 0 -1px 0 black, inset -1px 0 0 black;
  -moz-box-shadow: inset 0 -1px 0 black, inset -1px 0 0 black;
  box-shadow: inset 0 -1px 0 black, inset -1px 0 0 black;
}
#timeTable-control li div.time {
  line-height: 3em;
  background: #4e4e4e;
}
#timeTable-control li div.subject, #timeTable-control li div.class {
  background: #767676;
}
#timeTable-control li.selected div {
  background: #333333;
}
#timeTable-control li.selected div.subject, #timeTable-control li.selected div.class {
  background: #4e4e4e;
}

/**
*	Check Input
* *********************************************** */
.buttonArea {
  margin: 5px 0;
  text-align: right;
}

#inputCon {
  position: relative;
  padding: 3em 0 0;
}

#timeTable.check caption {
  display: block;
  width: 100%;
  height: 2em;
  line-height: 2em;
  position: absolute;
  top: 0;
  left: 0;
  color: #fff;
  text-align: center;
  background: #4da3d6;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  -ms-border-radius: 5px;
  -o-border-radius: 5px;
  border-radius: 5px;
}
#timeTable.check thead th {
  height: 5em;
  text-align: center;
}
#timeTable.check th.selected {
  background: #7aaceb;
  background: url('data:image/svg+xml;base64,');
  background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #3c77da), color-stop(49%, #407ee6), color-stop(50%, #4d89e8), color-stop(100%, #7aaceb));
  background: -webkit-linear-gradient(top, #3c77da, #407ee6 49%, #4d89e8 50%, #7aaceb);
  background: -moz-linear-gradient(top, #3c77da, #407ee6 49%, #4d89e8 50%, #7aaceb);
  background: -o-linear-gradient(top, #3c77da, #407ee6 49%, #4d89e8 50%, #7aaceb);
  -pie-background: linear-gradient(top, #3c77da, #407ee6 49%, #4d89e8 50%, #7aaceb);
  background: linear-gradient(top, #3c77da, #407ee6 49%, #4d89e8 50%, #7aaceb);
  -webkit-box-shadow: inset 0 7px 8px rgba(0, 0, 0, 0.37);
  -moz-box-shadow: inset 0 7px 8px rgba(0, 0, 0, 0.37);
  box-shadow: inset 0 7px 8px rgba(0, 0, 0, 0.37);
  text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.3);
  color: #fff;
}
#timeTable.check td.selected {
  background-image: url("../images/stripe-blue.gif");
}
#timeTable.check td {
  line-height: 1.5em;
  min-height: inherit;
  height: auto !important;
  height: inherit;
}
#timeTable.check #column1, #timeTable.check #column1 div {
  white-space: nowrap;
}
#timeTable.check #column2 {
  min-width: 130px;
  width: auto !important;
  width: 130px;
}
#timeTable.check #column3 {
  min-width: 365px;
  width: auto !important;
  width: 365px;
}
#timeTable.check #column4 {
  width: 200px;
}
#timeTable.check .diagram {
  width: 2em;
}
#timeTable.check .diagram span {
  padding: 0 3px;
}
#timeTable.check .diagram span.found-minus {
  color: #d00;
}
#timeTable.check .no {
  text-align: center;
  font-weight: 800;
  background: #fff;
}
#timeTable.check .even .no {
  background: #e8f4ff;
}
#timeTable.check .name span {
  display: block;
}
#timeTable.check .chk-group label {
  display: -moz-inline-stack;
  display: inline-block;
  vertical-align: middle;
  *vertical-align: auto;
  zoom: 1;
  *display: inline;
  padding: 0 6px;
  line-height: 2em;
  float: left;
  text-align: center;
  color: #333;
  background: #ccc;
  background: url('data:image/svg+xml;base64,');
  background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #fcfcfc), color-stop(100%, #cccccc));
  background: -webkit-linear-gradient(top, #fcfcfc, #cccccc);
  background: -moz-linear-gradient(top, #fcfcfc, #cccccc);
  background: -o-linear-gradient(top, #fcfcfc, #cccccc);
  -pie-background: linear-gradient(top, #fcfcfc, #cccccc);
  background: linear-gradient(top, #fcfcfc, #cccccc);
  text-shadow: 0 1px 0 white;
  -webkit-box-shadow: inset 1px 0 0 #f6f6f6;
  -moz-box-shadow: inset 1px 0 0 #f6f6f6;
  box-shadow: inset 1px 0 0 #f6f6f6;
  border-top: 1px solid #aaa;
  border-right: 1px solid #b5b6b5;
  border-bottom: 1px solid #999;
  cursor: pointer;
}
#timeTable.check .chk-group label:first-of-type {
  border-left: 1px solid #b5b6b5;
}
#timeTable.check .chk-group label.first {
  border-left: 1px solid #b5b6b5;
}
#timeTable.check .chk-group input[type="radio"] {
  display: none;
}
#timeTable.check .chk-group input[type="radio"]:checked + label {
  background: #7aaceb;
  background: url('data:image/svg+xml;base64,');
  background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #3c77da), color-stop(49%, #407ee6), color-stop(50%, #4d89e8), color-stop(100%, #7aaceb));
  background: -webkit-linear-gradient(top, #3c77da, #407ee6 49%, #4d89e8 50%, #7aaceb);
  background: -moz-linear-gradient(top, #3c77da, #407ee6 49%, #4d89e8 50%, #7aaceb);
  background: -o-linear-gradient(top, #3c77da, #407ee6 49%, #4d89e8 50%, #7aaceb);
  -pie-background: linear-gradient(top, #3c77da, #407ee6 49%, #4d89e8 50%, #7aaceb);
  background: linear-gradient(top, #3c77da, #407ee6 49%, #4d89e8 50%, #7aaceb);
  -webkit-box-shadow: inset 0 7px 8px rgba(0, 0, 0, 0.37);
  -moz-box-shadow: inset 0 7px 8px rgba(0, 0, 0, 0.37);
  box-shadow: inset 0 7px 8px rgba(0, 0, 0, 0.37);
  color: #fff;
  text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.3);
}
#timeTable.check .area {
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  -ms-border-radius: 5px;
  -o-border-radius: 5px;
  border-radius: 5px;
  height: 2em;
  padding: 3px;
  border: 1px solid #ccc;
  outline: none;
  -webkit-transition-duration: 0.2s;
  -moz-transition-duration: 0.2s;
  -o-transition-duration: 0.2s;
  transition-duration: 0.2s;
  -webkit-transition-timing-function: ease-out;
  -moz-transition-timing-function: ease-out;
  -o-transition-timing-function: ease-out;
  transition-timing-function: ease-out;
}
#timeTable.check .area:focus {
  height: 8em;
  background: #dbe2ed;
}
