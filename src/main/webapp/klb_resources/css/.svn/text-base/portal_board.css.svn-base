/*　[2] header, gnav
============================================================================ */
/* nav */
#page #navi nav #navi_menu li#btn_statistics a {
  background: url(../images/gnav_statistics.gif) no-repeat;
}
#page #navi nav #navi_menu li#btn_contact a {
  background: url(../images/gnav_contact_b.gif) no-repeat;
}
#page #navi nav #navi_menu li#btn_support a {width: 145px;}

/* hover */
#page #navi nav #navi_menu li#btn_statistics a:hover {
  background-image: url(../images/gnav_statistics_hover.gif);
}
#page #navi nav #navi_menu li#btn_contact a:hover {
  background-image: url(../images/gnav_contact_b_hover.gif);
}

/*　#schedule
------------------------------------------------- */
#contents_portal .boxwrap #schedule #tab .tablist a {
  display:table-cell;
  background-color: #fff;
  border-right: 1px solid #D3E3ED;
  padding: 10px;
  width:121px;
  font-size:13px;
  font-weight: bold;
  color: #000;
  text-decoration: none;
  line-height:1.3;
  vertical-align:middle;
}
#contents_portal .boxwrap #schedule #tab .tablist a.on,
#contents_portal .boxwrap #schedule #tab .tablist a:hover {
  border-right: 1px solid #6CAFD7;
  padding: 10px;
  font-weight: bold;
  color: #fff;
  text-decoration: none;
  background: #3295d0;
  background: url('data:image/svg+xml;base64,');
  background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #86c4e5), color-stop(100%, #3295d0));
  background: -webkit-linear-gradient(#86c4e5 0%, #3295d0 100%);
  background: -moz-linear-gradient(#86c4e5 0%, #3295d0 100%);
  background: -o-linear-gradient(#86c4e5 0%, #3295d0 100%);
  -pie-background: linear-gradient(#86c4e5 0%, #3295d0 100%);
  background: linear-gradient(#86c4e5 0%, #3295d0 100%);
}
#contents_portal .boxwrap #schedule dl {
	padding:8px 0;
	background-color:#fffadf;
	border-bottom:1px solid #cccccc;
	}
#contents_portal .boxwrap #schedule dl dt {
	padding:0 0 0 1em;
	width:4em;
	float:left;
	clear:both;
	vertical-align:top;
	}
#contents_portal .boxwrap #schedule dl dd {
	margin-left:5em;
	}
#contents_portal .boxwrap #schedule dl dd a:link,
#contents_portal .boxwrap #schedule dl dd a:visited {color:#000;}
#contents_portal .boxwrap #schedule dl dd a:hover {color:#3295d0;}

