/*　#document
------------------------------------------------- */
#document {
	margin:0;
	padding:0;
}
#document .variable_box_head p.variableadd {
	margin-top:6px;
	margin-right:15px;
	width:8em;
	float:right;
	vertical-align:middle;
	text-align:center;
}

#document table {
  width: 100%;
  clear: both;
}
#document table th {
  background-color: #0a9be1;
  color: #fff;
  font-size: 12px;
  text-align: center;
  table-layout:fixed;
}
#document table td {
  padding: 10px;
  table-layout:fixed;
}
#document table td a:link,
#document table td a:visited {
  color: #000;
}
#document table td a:hover {
  color: #3295d0;
}

#document table tr:nth-child(odd) {
  background-color: #DCF0FF;
}

#document table td:not(:last-child) {
  border-right: 1px solid #DCF0FF;
}

#document table tr:nth-child(odd) td:not(:last-child),
#document table th:not(:last-child) {
  border-right: 1px solid #fff;
}


/*	[4] sidemenu_portal
============================================================================ */
/* ボタン */
