package jp.go.saga.common;

public class GreeceWordReplace {
	public static String greeceWord(String str) {
		if(str == null)return "";
		String result = str;
		
	
		result = result.replaceAll("&Alpha;","Α");
		result = result.replaceAll("&alpha;","α");
		
		result = result.replaceAll("&Beta;","Β");
		result = result.replaceAll("&beta;","β");
		
		result = result.replaceAll("&Gamma;","Γ");
		result = result.replaceAll("&gamma;","γ");
		
		
		result = result.replaceAll("&Delta;","Δ");
		result = result.replaceAll("&delta;","δ");
		
		result = result.replaceAll("&Epsilon;","Ε");
		result = result.replaceAll("&epsilon;","ε");
		
		result = result.replaceAll("&Zeta;","Ζ");
		result = result.replaceAll("&zeta;","ζ");
		
		result = result.replaceAll("&Eta;","Η");
		result = result.replaceAll("&eta;","η");
		
		result = result.replaceAll("&Theta;","Θ");
		result = result.replaceAll("&theta;","θ");
		
		result = result.replaceAll("&Iota;","Ι");
		result = result.replaceAll("&iota;","ι");
		
		result = result.replaceAll("&Kappa;","Κ");
		result = result.replaceAll("&kappa;","κ");
		
		result = result.replaceAll("&Lambda;","Λ");
		result = result.replaceAll("&lambda;","λ");
		
		result = result.replaceAll("&Mu;","Μ");
		result = result.replaceAll("&mu;","μ");
		
		result = result.replaceAll("&Nu;","Ν");
		result = result.replaceAll("&nu;","ν");
		
		result = result.replaceAll("&Xi;","Ξ");
		result = result.replaceAll("&xi;","ξ");
		
		result = result.replaceAll("&Omicron;","Ο");
		result = result.replaceAll("&omicron;","ο");
		
		result = result.replaceAll("&Pi;","Π");
		result = result.replaceAll("&pi;","π");
		
		result = result.replaceAll("&Rho;","Ρ");
		result = result.replaceAll("&rho;","ρ");
		
		result = result.replaceAll("&Sigma;","Σ");
		result = result.replaceAll("&sigma;","σ");
		
		result = result.replaceAll("&Tau;","Τ");
		result = result.replaceAll("&tau;","τ");
		
		result = result.replaceAll("&Upsilon;","Υ");
		result = result.replaceAll("&upsilon;","υ");
		
		result = result.replaceAll("&Phi;","Φ");
		result = result.replaceAll("&phi;","φ");
		
		result = result.replaceAll("&Chi;","Χ");
		result = result.replaceAll("&chi;","χ");
		
		result = result.replaceAll("&Psi;","Ψ");
		result = result.replaceAll("&psi;","ψ");
		
		result = result.replaceAll("&Omega;","Ω");
		result = result.replaceAll("&omega;","ω");
		
	
		
		return result;
	}
}
